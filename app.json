{
  "id": "com.wikipedia",
  "version": "2.0.0",
  "compatibility": ">=0.8.30",
  "category": "internet",
  "name": {
    "en": "Wikipedia",
    "nl": "Wikipedia"
  },
  "description": {
    "en": "Ask Homey to get you infromation from Wikipedia",
    "nl": "Vraag informatie op van Wikipedia"
  },
  "author": {
    "name": "Athom B.V."
  },
  "images": {
    "large": "./assets/images/large.jpg",
    "small": "./assets/images/small.jpg"
  },
  "dependencies": {
  },
  "permissions": [
    "homey:manager:speech-output",
    "homey:manager:speech-input"
  ],
  "flow": {
    "triggers": [],
    "conditions": []
  },
  "speech": [
    {
      "id": "wikipedia",
      "importance": 1,
      "triggers": {
        "en": [
          "on wikipedia",
          "wikipedia",
          "wiki"
        ],
        "nl": [
          "op wikipedia naar",
          "op wikipedia",
          "wikipedia",
          "wiki"
        ]
      }
    },
    {
      "id": "wikipedia_alternative",
      "importance": 0.5,
      "triggers": {
        "en": [
          "every thing yeah",
          "when will the media",
          "bring it up here",
          "what yeah",
          "information about"
        ],
        "nl": [
          "informatie over",
          "vertel me over",
          "vertel me iets over",
          "vertel iets over",
          "wat weet je over",
          "wat weet je van"
        ]
      }
    },
    {
      "id": "support_word",
      "importance": false,
      "triggers": {
        "en": [
          "look up",
          "lookup",
          "find",
          "search for",
          "search"
        ],
        "nl": [
          "zoek",
          "zoek naar",
          "vind"
        ]
      }
    }
  ]
}